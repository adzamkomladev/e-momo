{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@ejara/xyz/(.*)": "<rootDir>/../libs/xyz/src/$1",
    "@ejara/xyz": "<rootDir>/../libs/xyz/src",
    "@ejara/utils/(.*)": "<rootDir>/../libs/utils/src/$1",
    "@ejara/utils": "<rootDir>/../libs/utils/src"
  }
}